[supervisord]
logfile=/var/log/supervisor/supervisord.log ; (main log file;default $CWD/supervisord.log)
pidfile=/var/run/supervisord.pid ; (supervisord pidfile;default supervisord.pid)
childlogdir=/var/log/supervisor            ; ('AUTO' child log dir, default $TEMP)
nodaemon=%(ENV_GADGETRON_SUPERVISOR_NODAEMON)s
environment=LD_LIBRARY_PATH="/usr/local/nvidia/lib:/usr/local/nvidia/lib64:/usr/local/cuda/lib64:/usr/local/cuda/lib64/stubs:/usr/lib/x86_64-linux-gnu:/usr/local/lib:/opt/intel/mkl/lib/intel64:/opt/intel/lib/intel64:@CMAKE_INSTALL_PREFIX@/lib:@CMAKE_INSTALL_PREFIX@/lib_extra:/python",GADGETRON_HOME="@CMAKE_INSTALL_PREFIX@",ISMRMRD_HOME="/usr/local"

[unix_http_server]
file=/var/run/supervisor.sock   ; (the path to the socket file)
chmod=0700                       ; sockef file mode (default 0700)

[rpcinterface:supervisor]
supervisor.rpcinterface_factory = supervisor.rpcinterface:make_main_rpcinterface

[supervisorctl]
serverurl=unix:///var/run/supervisor.sock ; use a unix:// URL  for a unix socket

[inet_http_server]
port=9001

[program:gadgetron]
stderr_logfile=/gadgetron_log/gadgetron_err.log
stdout_logfile=/gadgetron_log/gadgetron.log
autorestart=true
command=@CMAKE_INSTALL_PREFIX@/bin/gadgetron -r %(ENV_GADGETRON_RELAY_HOST)s

[program:gadgetron_cloudbus_relay]
stderr_logfile=/gadgetron_log/gadgetron_relay_err.log
stdout_logfile=/gadgetron_log/gadgetron_relay.log
autorestart=true
command=@CMAKE_INSTALL_PREFIX@/bin/gadgetron_cloudbus_relay
